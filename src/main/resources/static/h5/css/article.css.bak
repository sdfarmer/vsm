.ad360 {
    width: 100%;
    border-top: 3px solid #f3f3f3;
    border-bottom: 3px solid #f3f3f3;
    position: relative;
    padding-top:8px;
    padding-bottom:10px;
}
.adtitle {
    width: 92%;
    margin: 0 auto;
    font-size: 16px;
    font-weight: bold;
    color: #333;
    position: relative;
    line-height: 22px;
    padding-left: 10px;
    margin-bottom: 5px;
    position:relative;
    margin-bottom:10px;
}
.adtitle:before {
    content: "";
    display: inline-block;
    background: #577df8;
    width: 3px;
    height: 15px;
    position: absolute;
    top: 5px;
    left: 0px;
    z-index: 1;
}
.ad360 a {
    width: 92%;
    display: block;
    margin: 0 auto;
    box-shadow:0px 0px 5px #ccc;
    margin-bottom:8px;
    padding:0 2%;
    padding-bottom:6px;
}
   
