.fenlei {
    width: 94%;
    margin: 0 auto;
    padding:0 3%;
    background: #fff;
}

    .fenlei ul {
        width: 94%;
        margin: 0 auto;
    }

        .fenlei ul li {
            overflow: hidden;
            padding-bottom: 10px;
        }

            .fenlei ul li.rightimg div {
                float: left;
                width: 67%;
                margin-right: 3%;
            }

            .fenlei ul li.rightimg img {
                float: left;
                width: 30%;
                border-radius: 3px;
                margin-top: 5px;
            }

            .fenlei ul li.oneimg img {
                width: 100%;
                display: block;
                margin: 0 auto;
                border-radius: 3px;
                margin-top: 5px;
            }

            .fenlei ul li.threeimg img {
                float: left;
                width: 32%;
                margin-right: 2%;
                border-radius: 3px;
                margin-top: 5px;
            }

                .fenlei ul li.threeimg img:nth-child(3) {
                    margin-right: 0;
                }

            .fenlei ul li.threeimg .imgdiv {
                overflow: hidden;
                margin: 10px auto;
            }

    .fenlei h2 {
        width: 97%;
        font-size: 20px;
        height: 40px;
        line-height: 40px;
        font-weight: bold;
        padding-top: 15px;
        /*padding-left:20px;*/
        margin-bottom: 10px;
        /*color: #577df8;*/
    }

        .fenlei h2 b {
            float: left;
            width: 3px;
            height: 18px;
            margin-right: 10px;
            background: #577df8;
            margin-top: 13px;
        }

        .fenlei h2 span {
            float: right;
        }

            .fenlei h2 span a {
                float: left;
                height: 40px;
                line-height: 40px;
                padding: 0 8px;
                font-size: 14px;
                color: #8e8e8e;
                font-weight: normal;
            }

    .fenlei ul li {
        border-bottom: 1px solid #eeeef6;
        margin-bottom: 8px;
        padding-bottom: 16px;
        padding-top: 10px;
    }
        .fenlei ul li:last-child{
            border-bottom:0;
            margin-bottom:0;
        }
        .fenlei ul li div p {
            line-height: 30px;
            font-size: 18px;
            color: #080808;
        }

            .fenlei ul li div p span {
                float: left;
                height: 22px;
                line-height: 22px;
                padding: 0px 5px;
                background: #e45a3b;
                color: #fff;
                text-align: center;
                font-size: 14px;
                border-radius: 3px;
                margin-top: 4px;
                margin-right: 3px;
            }

        .fenlei ul li div span s {
            font-size: 14px;
            margin-right: 10px;
            text-decoration: none;
            color: #8e8e8e;
        }

