            }

        .fenlei ul li div span s {
            font-size: 14px;
            margin-right: 10px;
            text-decoration: none;
            color: #a1a8b3;
        }

.ad360 {
    width: 100%;
    position: relative;
    padding-top:8px;
    padding-bottom:10px;
}
.adtitle {
    width: 92%;
    margin: 0 auto;
    font-size: 16px;
    font-weight: bold;
    color: #333;
    position: relative;
    line-height: 22px;
    padding-left: 10px;
    margin-bottom: 5px;
    position:relative;
    margin-bottom:10px;
}
.adtitle:before {
    content: "";
    display: inline-block;
    background: #577df8;
    width: 3px;
    height: 15px;
    position: absolute;
    top: 5px;
    left: 0px;
    z-index: 1;
}
.ad360 a {
    width: 94%;
    display: block;
    margin: 0 auto;
    margin-bottom:8px;
    padding:0 3%;
    padding-bottom:6px;
    background: #fff;
}
.top_ad .zujian {
    position: absolute;
    right: 0;
    bottom: 3px;
    color: #fff;
}

    .top_ad .zujian span {
        background: #e2e2e2;
        margin-right: 3px;
        height: 14px;
        line-height: 14px;
        float: left;
        font-size: 10px;
    }

    .top_ad .zujian .yiwen_360 {
        width: 14px;
        border-radius: 10px;
        text-align: center;
    }

    .top_ad .zujian .close_top {
        width: 14px;
        border-radius: 10px;
        text-align: center;
        margin-right: 0;
        line-height: 11px;
    }
    .back {
    position: fixed;
    right: 1%;
    bottom: 40px;
    z-index: 88;
}

    .back img {
        width: 45px;
        display: block;
        margin: 0 auto;
    }

.swhz {
    position: fixed;
    right: 1%;
    bottom: 90px;
    background: #577df8;
    border-radius:46px;
}
    .swhz span {
        display: block;
        width: 36px;
        color: #577df8;
        text-align: center;
        font-size: 14px;
        border-radius: 100%;
        border: 1px solid #577df8;
        background: #fff;
        line-height:18px;
        font-weight:bold;
        padding:4px;
        float:right;
        position:relative;
        z-index:3;
    }
    .swhz p {
        color: #fff;
        font-size: 14px;
        width: 80px;
        float: left;
        padding-left: 12px;
        line-height: 20px;
        padding-top: 3px;
        position: absolute;
        top: 0;
        right: -200px;
        background: #577df8;
        border-radius: 46px;
        padding-right:46px;
        z-index:2;
        height:43px;
       
    }
    .ad_list{
        overflow: hidden;
        padding-top: 6px;
    }

    .ad_logo{
width: 13%;
float: left;
margin-left: 1%;
margin-right: 3%;
    }
        .ad_des{
width: 58%;
float: left;
    }
        .ad_apply{
width: 25%;
float: left;
        height: 24px;
        line-height: 24px;
        text-align: center;
        color: #fff;
        background: #FF5003;
        border-radius: 50px;
        font-size: 14px;
        margin-top: 25px;
        text-decoration: none;
    }
     .ad_des h3 {
        font-size: 16px;
        color: #333;
        margin-right:5px;
    }
        .ad_des .limit {
            color:red;
            margin-top:2px;
            font-size:18px;
        }
              .ad_des .limit span{
                color: #a1a8b3;
                font-size: 14px;
                margin-left: 5px;
              }
                  .ad_des .item{
                color: #a1a8b3;
                font-size: 14px;
              }
               .ad_des .item s{
                text-decoration: none;
                margin-right: 5px;
               }

.hot{
      overflow: hidden;
      margin-bottom: 15px;
               }
.hot a{
    width: 32%;
    margin-right: 2%;
    float: left;
    box-sizing: border-box;
    background: #fff;
    padding-top: 10px;
    padding-bottom: 10px;
    margin-bottom: 10PX;
}
.hot a:nth-child(3n+1){
    margin-right: 0;
}
.hot .hot_logo{
    width: 34%;
    display: block;
    margin: 0 auto;
    margin-bottom:7px;
}
.hot_list h3{
    font-size: 16px;
    font-weight: bold;
    color: #333;
    text-align: center;
    margin-bottom: 2px;
}
.hot_des .limit {
    color: red;
    margin-top: 2px;
    font-size: 18px;
    text-align: center;
    margin-bottom: 2px;
}
.hot_des .limit span {
    color: #a1a8b3;
    font-size: 14px;
    margin-left: 5px;
    display: block;
    text-align: center;

}
.hot_des .item {
    color: #a1a8b3;
    font-size: 14px;
    text-align: center;
    display: block;
    margin-bottom: 8px;
}
.hot_apply {
    width: 85px;
    height: 22px;
    display: block;
    margin: 0 auto;
    line-height: 22px;
    text-align: center;
    color: #FF5003;
    border-radius: 50px;
    font-size: 14px;
    text-decoration: none;
    border: 1px solid #FF5003;
    margin-top: 15px;
}