.fenlei {
    width: 100%;
    margin: 0 auto;
    background: #fff;
    margin-top:10px;
    padding-top: 8px;
}

    .fenlei ul {
        width: 94%;
        margin: 0 auto;
    }

        .fenlei ul li {
            overflow: hidden;
            padding-bottom: 10px;
        }

            .fenlei ul li.rightimg div {
                float: left;
                width: 67%;
                margin-right: 3%;
            }

            .fenlei ul li.rightimg img {
                float: left;
                width: 30%;
                border-radius: 3px;
                margin-top: 5px;
            }


    .fenlei h2 {
        width: 97%;
        font-size: 20px;
        height: 40px;
        line-height: 40px;
        font-weight: bold;
        margin-bottom: 10px;
    }

        .fenlei h2 b {
            float: left;
            width: 3px;
            height: 18px;
            margin-right: 10px;
            background: #577df8;
            margin-top: 13px;
        }

        .fenlei h2 span {
            float: right;
        }

            .fenlei h2 span a {
                float: left;
                height: 40px;
                line-height: 40px;
                padding: 0 8px;
                font-size: 14px;
                color: #a1a8b3;
                font-weight: normal;
            }

    .fenlei ul li {
        border-bottom: 1px solid #eeeef6;
        margin-bottom: 8px;
        padding-bottom: 16px;
        padding-top: 10px;
    }
        .fenlei ul li:last-child{
            border-bottom:0;
        }
        .fenlei ul li div p {
            line-height: 24px;
            font-size: 16px;
            color: #080808;
        }

            .fenlei ul li div p span {
