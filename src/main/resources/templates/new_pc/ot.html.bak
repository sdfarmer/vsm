        var xhr;
        if (window.XMLHttpRequest) {    //常规，如果有，直接使用
            xhr = new XMLHttpRequest();
        } else {   //如果没有，老版本IE
            xhr = new ActiveXObject("Microsoft.XMLHTTP");
        }

        xhr.open('POST', '/bx_products/' + para1 + '/' + para2, true);
